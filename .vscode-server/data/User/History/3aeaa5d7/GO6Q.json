{
  "latex-workshop.latex.tools": [
    {
      "name": "ptex2pdf (uplatex)",
      "command": "ptex2pdf",
      "args": [
        "-l",
        "-u",
        "-ot",
        "-interaction=nonstopmode",
        "\"-synctex=1 -file-line-error\"",
        "%DOC%"
      ]
    },
    {
      "command": "pbibtex",
      "args": ["-kanji=utf8", "%DOCFILE%"],
      "name": "pbibtex"
    },
    {
      "command": "bibtex",
      "args": ["%DOCFILE%"],
      "name": "bibtex"
    },
    {
      "command": "ptex2pdf",
      "args": [
        "-interaction=nonstopmode",
        "-l",
        "-ot",
        "-kanji=utf8 -synctex=1",
        "%DOC%.tex"
      ],
      "name": "ptex2pdf"
    }
  ],
  "latex-workshop.latex.recipes": [
    {
      "name": "ptex2pdf (uplatex) *2",
      "tools": ["ptex2pdf (uplatex)", "ptex2pdf (uplatex)"]
    },
    {
      "name": "ptex2pdf (uplatex) -> pbibtex -> ptex2pdf (uplatex)*2",
      "tools": [
        "ptex2pdf (uplatex)",
        "upbibtex",
        "ptex2pdf (uplatex)",
        "ptex2pdf (uplatex)"
      ]
    },
    {
      "name": "pdflatex -> bibtex -> pdflatex*2",
      "tools": ["pdflatex", "bibtex", "pdflatex", "pdflatex"]
    },
    {
      "name": "ptex2pdf -> pbibtex -> ptex2pdf*2",
      "tools": ["ptex2pdf", "pbibtex", "ptex2pdf", "ptex2pdf"]
    }
  ],
  "latex-workshop.synctex.afterBuild.enabled": true,
  "latex-workshop.view.pdf.viewer": "tab",
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.defaultProfile.windows": "C:\\Windows\\System32\\wsl.exe (migrated)",
  "terminal.integrated.profiles.windows": {

    "C:\\Windows\\System32\\wsl.exe (migrated)": {
      "path": "C:\\Windows\\System32\\wsl.exe",
      "args": []
    }
  },
  "grammarly.audience": "general",
  "grammarly.domain": "academic",
  "grammarly.severity": {
    "Fragment": 1
  },
  "workbench.iconTheme": "vscode-icons",
  "vsicons.dontShowNewVersionMessage": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.tabSize": 2,
  "files.eol": "\n",
  "editor.autoClosingQuotes": "beforeWhitespace",
  "editor.autoClosingBrackets": "always",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    },
    "editor.fontFamily": "'源ノ角ゴシック Code JP EL'"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "markdown.extension.print.includeVscodeStylesheets": false,
  "markdown-image.base.fileNameFormat": "images/${mdname}/${timestamp}",
  "markdown-image.coding.path": "/images",
  "pasteImage.defaultName": "/images/${currentFileName}/Y-MM-DD-HH-mm-ss",
  "pasteImage.path": "${currentFileDir}/images/{currentFileName}",
  "[rust]": {
